{
  "name": "saiHTML",
  "description": "Removedor de Elementos HTML",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ], 
  "action": {
    "browser_style": true,
    "default_title": "saiHTML",
    "default_popup": "index.html"
  }
}